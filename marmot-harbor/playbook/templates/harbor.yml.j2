hostname: harbor.labo.local
http:
  port: 80
https:
  port: 443
  certificate: /harbor/certs/harbor.labo.local.crt
  private_key: /harbor/certs/harbor.labo.local.key
#internal_tls:
#  enabled: true
#  dir: /harbor/certs
harbor_admin_password: Harbor12345
database:
  password: root123
  max_idle_conns: 50
  max_open_conns: 1000
data_volume: /harbor/data
clair:
  updaters_interval: 12
trivy:
  ignore_unfixed: false
  skip_update: false
  insecure: false
jobservice:
  max_job_workers: 10
notification:
  webhook_job_max_retry: 10
chart:
  absolute_url: disabled
log:
  level: info
  local:
    rotate_count: 50
    rotate_size: 200M
    location: /harbor/log
_version: 2.0.0
proxy:
  http_proxy:
  https_proxy:
  no_proxy:
  components:
    - core
    - jobservice
    - clair
    - trivy
